@import "../../hud/hud.wxss";

/**index.wxss**/

.my-srcoll {
  height: calc(100% - 350rpx);
}

.my-head {
  width: 100%;
  height: 128rpx;
}

.car-code-num {
  padding: 15rpx 25rpx;
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #9b9b9b;
  letter-spacing: 0.52px;
  display: block;
  margin-top: 20rpx;
  background: #fff;
}

.car-goods-title {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #bbb;
  letter-spacing: 0.52px;
  text-align: center;
}

.addr-name {
  font-family: PingFangSC-Medium;
  font-size: 35rpx;
  color: #4a4a4a;
  letter-spacing: 0.67px;
  text-align: center;
  white-space: nowrap;
  position: relative;
  left: 0;
  padding: 5rpx 30rpx;
}

@keyframes mymove {
  0% {
    left: 0px;
  }

  50% {
    left: 200px;
  }

  100% {
    left: 0px;
  }
}

page {
  height: 100%;
  background-color: #f8f8f8;
}

.detail-message {
  background: #fff;
  padding: 10rpx 30rpx;
}

.order_process {
  display: flex;
  flex-wrap: nowrap;
  padding: 20rpx 10rpx 30rpx 10rpx;
  /* padding: 10rpx 10rpx 20rpx 10rpx; */
  background-color: #fff;
  position: relative;
  top: -11rpx;
}

.process_wrap {
  display: flex;
  flex-direction: column;
  flex: 1;
  align-items: center;
}

.process {
  display: flex;
  align-items: center;
  width: 100%;
  margin-top: 5px;
}

.process_icon {
  width: 25rpx;
  height: 25rpx;
}

.process_line {
  background: #aaa;
  flex: 1;
  height: 5rpx;
}

.process_name {
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  letter-spacing: 0.52px;
  position: absolute;
  color: #fff;
  /* display: inline-block;
  margin-left: 116rpx;
  margin-top: -23px; */
}

.show-tickt {
  background: #f4f8ff;
  padding: 20rpx 0;
}

.load-num {
  width: 50%;
  display: inline-block;
  background: #fff;
}

.load-num .my-left {
  padding: 10rpx 30rpx;
}

.load-num .my-right {
  padding: 10rpx 30rpx;
  text-align: right;
}

.expand-open {
  background: #fff;
  text-align: center;
  margin-top: 10rpx;
}

.expand-close {
  background: #fff;
  text-align: center;
  margin-top: 10rpx;
  transform: rotate(180deg);
}

.my-text {
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #9b9b9b;
  letter-spacing: 2rpx;
}

.my-dd-phone {
  display: flex;
  flex-wrap: nowrap;
  padding: 20rpx 0;
  background: #fff;
}

.enter-goods-num {
  background: #fff;
  padding: 10rpx 50rpx;
  margin-top: 10rpx;
}

.dd-input {
  padding: 5rpx 10rpx;
  display: inline-block;
  width: 250rpx;
  vertical-align: middle;
}

.dd-input-placeholder {
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #b3cffd;
  letter-spacing: 0.52px;
}

.my-upload {
  padding: 30rpx 60rpx;
  background: #fff;
  text-align: center;
  margin-top: 10rpx;
  position: relative;
}

.upload-zoom {
  position: absolute;
  height: 60rpx;
  width: 100%;
  opacity: 0.7;
  background: #000;
  bottom: 0rpx;
  color: white;
}

.my-upload-img {
  position: relative;
}

.upload-img-close {
  position: absolute;
  right: 0;
  top: 6rpx;
  right: 40rpx;
}

.my-upload-zoom-text {
  position: relative;
  z-index: 1;
  opacity: 0.8;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #fff;
  letter-spacing: 0.44px;
  top: 10rpx;
}

.new-login-enter {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 150rpx;
  background: #fff;
  padding: 20rpx 0;
  z-index: 100;
}

.my-dd-jd {
  display: flex;
  flex-wrap: nowrap;
  margin-top: 40rpx;
}

.login-enter {
  position: relative;
  width: 100%;
  bottom: 0;
}

.button {
  height: 90rpx;
  /* background-color: #00A0FF; */
  color: #fff;
  width: calc(100% - 80rpx);
  margin-left: 40rpx;
}

button[class="enter"]::after {
  border: 0;
}

.enter {
  position: absolute;
  text-align: center;
  color: #fff;
  z-index: 0;
  width: calc(100% - 80rpx);
  left: 50%;
  transform: translate(-50%, 0);
  height: 90rpx;
  top: 22rpx;
  background: rgba(255, 255, 255, 0);
  border-width: 0;
  border: none;
}

.jd-enter {
  position: absolute;
  color: #fff;
  z-index: 0;
  width: calc(100% - 80rpx);
  left: 50%;
  transform: translate(-50%, 0);
  height: 80rpx;
  background: rgba(255, 255, 255, 0);
  border-width: 0;
  border: none;
  top: 0;
  font-family: PingFangSC-Medium;
  font-size: 30rpx;
  letter-spacing: 0.59px;
  text-align: center;
}
